{
  "name": "NYC Trip Planner",
  "description": "Intrepid explorer with ChatGPT and AIConfig",
  "schema_version": "latest",
  "metadata": {
    "models": {
      "gpt2": {
        "model": "gpt2",
        "max_tokens": 3000,
        "system_prompt": "Some random system prompt"
      },
      "runwayml/stable-diffusion-v1-5": {
        "model": "runwayml/stable-diffusion-v1-5"
      },
      "stabilityai/stable-diffusion-xl-base-1.0": {
        "model": "stabilityai/stable-diffusion-xl-base-1.0"
      }
    },
    "model_parsers": {
      "runwayml/stable-diffusion-v1-5": "HuggingFaceText2ImageDiffusor"
    },
    "default_model": "gpt2"
  },
  "prompts": [
    {
      "name": "gen_image",
      "input": "a cute {{animal}} holding 2 AKs with an explosion behind it",
      "metadata": {
        "parameters": {
          "animal": "bunny"
        }
      }
    },
    {
      "name": "gen_image_sdxl",
      "input": "a cute {{animal}} holding 2 AKs with an explosion behind it",
      "metadata": {
        "model": {
          "name": "stabilityai/stable-diffusion-xl-base-1.0",
          "settings": {
            "num_images_per_prompt": 2,
            "num_inference_steps": 50
          }
        },
        "parameters": {
          "animal": "bunny"
        }
      }
    }
  ]
}


