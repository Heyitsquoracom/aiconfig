{
  "name": "Text Generation Test",
  "description": "Intrepid explorer with ChatGPT and AIConfig",
  "schema_version": "latest",
  "metadata": {
    "models": {
      "fake_model": {
        "model": "mistralai/Mistral-7B-v0.1",
        "max_tokens": 3000,
        "system_prompt": "You are an expert travel coordinator with exquisite taste."
      }
    },
    "model_parsers": {
      "mistralai/Mistral-7B-v0.1": "HuggingFaceTextGenerationTransformer"
    }
  },
  "prompts": [
    {
      "name": "gen_story",
      "input": "I was walking in {{place}} when all of a sudden",
      "metadata": {
        "model": {
          "name": "mistralai/Mistral-7B-v0.1",
          "settings": {
              "num_return_sequences": 1,
              "max_new_tokens": 200,
              "min_new_tokens": 190
            }
          },
        "parameters": {
          "place": "New York"
        }
      }
    }
  ]
}


